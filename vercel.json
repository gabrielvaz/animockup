{
  "version": 2,
  "builds": [
    {
      "src": "src/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.php",
      "use": "vercel-php@0.7.1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ],
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  }
}